    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="ladoquadrado, areaquadrado, lado1retangulo, lado2retangulo, arearetangulo, triangulo1, triangulo2, areatriangulo" type="Integer" array="False" size=""/>
            <input variable="ladoquadrado"/>
            <assign variable="areaquadrado" expression="ladoquadrado*ladoquadrado"/>
            <output expression="&quot;Area quadrado = &quot;&amp;areaquadrado" newline="True"/>
            <input variable="lado1retangulo"/>
            <input variable="lado2retangulo"/>
            <assign variable="arearetangulo" expression="lado1retangulo*lado2retangulo"/>
            <output expression="&quot;Area retangulo = &quot;&amp; arearetangulo" newline="True"/>
            <input variable="triangulo1"/>
            <input variable="triangulo2"/>
            <assign variable="areatriangulo" expression="(triangulo1*triangulo2)/2"/>
            <output expression="&quot;Resultado triangulo = &quot;&amp; areatriangulo" newline="True"/>
        </body>
    </function>
